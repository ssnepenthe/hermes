{
  "name": "ssnepenthe/hermes",
  "description": "A configuration based web scraper.",
  "type": "library",
  "require": {
    "php": ">=7.0",
    "symfony/config": "^3.2",
    "symfony/css-selector": "^3.2",
    "symfony/dom-crawler": "^3.2",
    "symfony/yaml": "^3.2"
  },
  "require-dev": {
    "mockery/mockery": "^0.9.8",
    "phpunit/phpunit": "^6.0",
    "psy/psysh": "^0.8",
    "squizlabs/php_codesniffer": "^2.8",
    "symfony/console": "^3.2"
  },
  "license": "GPL-2.0",
  "autoload": {
    "files": ["inc/functions.php"],
    "psr-4": { "SSNepenthe\\Hermes\\": "src/" }
